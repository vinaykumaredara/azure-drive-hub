{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "importOrder": [
    "^react$",
    "^react-dom$",
    "^@?\\w",
    "^@/",
    "^\\.\\.?/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}