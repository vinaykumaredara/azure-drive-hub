{
  "name": "rp-cars",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:preflight && vite",
    "dev:mobile": "npm run dev:preflight && vite --host 0.0.0.0 --port 5173 --open",
    "dev:preflight": "npx kill-port 5173 || true",
    "clean:cache": "rimraf node_modules/.vite",
    "dev:clean": "npm run dev:preflight && vite",
    "build": "vite build",
    "build:analyze": "vite build --config scripts/analyze-bundle.js",
    "build:dev": "vite build --mode development",
    "build:prod": "npm run lint:fix && npm run type-check && vite build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json,css,md}'",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --run integration",
    "test:watch": "vitest --watch",
    "test:ci": "vitest --run --coverage",
    "test:smoke": "node scripts/smoke-test.js",
    "gen:supabase-types": "node scripts/generate-supabase-types.js",
    "verify:rls": "node scripts/verify-rls-policies.js",
    "verify:final": "node scripts/final-verification.js",
    "audit:security": "npm audit --audit-level moderate",
    "precommit": "npm run lint:fix && npm run type-check && npm run test:ci"
  },
